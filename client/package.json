{
  "name": "afps-client",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "bash ../tools/sync_weapon_assets.sh",
    "prebuild": "bash ../tools/sync_weapon_assets.sh",
    "pretest": "bash ../tools/sync_weapon_assets.sh",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"**/*.ts\" --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "playwright test -c playwright.config.ts",
    "test:ui:prejoin": "playwright test -c playwright.config.ts tests/ui/prejoin_preview.spec.ts",
    "perf:check": "PERF_CHECK=1 vitest run --config vitest.perf.config.ts",
    "wasm:build": "bash ../tools/wasm_sync.sh",
    "wasm:parity": "node ../tools/wasm_parity_check.mjs",
    "wasm:check": "npm run wasm:build && npm run wasm:parity",
    "test:ui:prejoin-join": "playwright test -c playwright.config.ts tests/ui/prejoin_join_button.spec.ts",
    "test:mutation": "stryker run"
  },
  "dependencies": {
    "flatbuffers": "^24.3.25",
    "three": "^0.164.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@stryker-mutator/core": "^8.4.0",
    "@stryker-mutator/vitest-runner": "^8.4.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.0",
    "fast-check": "^3.16.0",
    "globals": "^15.0.0",
    "happy-dom": "^20.4.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
